<%=form_for(:session, url: login_path) do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %><br/>

  <%= f.label :password %>
  <%= f.password_field :password %><br/>

  <%= f.label :remember_me %>
  <%= f.check_box :remember_me %><br/>

  <%= f.submit "Login" %>

<% end %>
<%= link_to "New User?", signup_path %>
<%= link_to "Users list", users_path %>